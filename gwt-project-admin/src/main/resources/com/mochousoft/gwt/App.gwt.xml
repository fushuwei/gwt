<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="app">
    <!-- GWT Boot stuffs -->
    <inherits name="com.github.gwtboot.starter.Starter"/>

    <!-- Domino UI components stuffs -->
    <inherits name="org.dominokit.domino.ui.DominoUI"/>
